{
  "version": 2,
  "functions": {
    "dist/main.js": {
      "runtime": "@vercel/node@18.x"
    }
  },
  "routes": [
    {
      "src": "/api/docs/(.*)",
      "dest": "/dist/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/main.js"
    }
  ],
  "env": {
    "ENABLE_SWAGGER": "true"
  },
  "buildCommand": "npm run build"
}
